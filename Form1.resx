    <value>// Vertex Shader
// OpenGL v3.3
// GLSL v3.30
// Legacy Shader
// For geometry tests without materials
// Vertices Data: Vertex, Normal, Color
// Matrices: MVP, Normal (inverse transpose of modelview_matrix: same as modelview without scaling)
// Light: Ambient, Diffuse, Orientation (non-spot, non-point)
// Per-vertex colors and ambient / diffuse lighting.
#version 330
//out vec4 gl_Position; Actual and useless declaration of mandatory output from vertex shader.

//Geometry Input Data
layout(location = 0) in vec3 in_vertex;
layout(location = 1) in vec3 in_normal;
layout(location = 2) in vec4 in_color;

//Matrices
uniform mat4 matrix_modelviewprojection;
uniform mat4 matrix_normal; 

//Light
uniform vec4 light_ambient;
uniform vec4 light_diffuse;
uniform vec4 light_orientation;

out vec4 var_color;

void main()
{
	vec4 buf_normal = normalize(matrix_normal * vec4(in_normal, 0.0));	
	float fact_diffuse = max(dot(light_orientation, buf_normal), 0.0);
	var_color = clamp((light_ambient * in_color) + ((fact_diffuse * light_diffuse) * in_color), 0.0, 1.0);	
	gl_Position = matrix_modelviewprojection * vec4(in_vertex, 1.0);
}</value>
  </data>
